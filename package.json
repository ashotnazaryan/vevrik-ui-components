{
  "name": "@vevrik/ui-components",
  "version": "0.1.11",
  "description": "Reusable React UI components for Vevrik platform built with MUI",
  "author": {
    "name": "Ashot",
    "url": "https://github.com/ashotnazaryan/vevrik-ui-components"
  },
  "license": "MIT",
  "homepage": "https://github.com/ashotnazaryan/vevrik-ui-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashotnazaryan/vevrik-ui-components.git"
  },
  "bugs": {
    "url": "https://github.com/ashotnazaryan/vevrik-ui-components/issues"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "mui",
    "ui",
    "vevrik",
    "components",
    "library"
  ],
  "scripts": {
    "build": "npm run generate:exports && tsup && tsc",
    "generate:exports": "node ./scripts/generate-exports.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json,html,md,yml,yaml,scss}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,json,html,md,yml,yaml,scss}'",
    "prepare": "husky",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "@mui/icons-material": "^7",
    "@mui/material": "^7",
    "react": "^18 || ^19",
    "react-hook-form": "^7"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,scss,md}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@types/react": "^18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "react-hook-form": "^7.66.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./CircularLoader": {
      "types": "./dist/CircularLoader/index.d.ts",
      "import": "./dist/CircularLoader/index.js",
      "require": "./dist/CircularLoader/index.cjs"
    },
    "./CopyToClipboard": {
      "types": "./dist/CopyToClipboard/index.d.ts",
      "import": "./dist/CopyToClipboard/index.js",
      "require": "./dist/CopyToClipboard/index.cjs"
    },
    "./Details": {
      "types": "./dist/Details/index.d.ts",
      "import": "./dist/Details/index.js",
      "require": "./dist/Details/index.cjs"
    },
    "./FileUpload": {
      "types": "./dist/FileUpload/index.d.ts",
      "import": "./dist/FileUpload/index.js",
      "require": "./dist/FileUpload/index.cjs"
    },
    "./Form": {
      "types": "./dist/Form/index.d.ts",
      "import": "./dist/Form/index.js",
      "require": "./dist/Form/index.cjs"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helpers/index.js",
      "require": "./dist/helpers/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./LinearLoader": {
      "types": "./dist/LinearLoader/index.d.ts",
      "import": "./dist/LinearLoader/index.js",
      "require": "./dist/LinearLoader/index.cjs"
    },
    "./LoadingButton": {
      "types": "./dist/LoadingButton/index.d.ts",
      "import": "./dist/LoadingButton/index.js",
      "require": "./dist/LoadingButton/index.cjs"
    },
    "./Page": {
      "types": "./dist/Page/index.d.ts",
      "import": "./dist/Page/index.js",
      "require": "./dist/Page/index.cjs"
    },
    "./Snackbar": {
      "types": "./dist/Snackbar/index.d.ts",
      "import": "./dist/Snackbar/index.js",
      "require": "./dist/Snackbar/index.cjs"
    },
    "./Table": {
      "types": "./dist/Table/index.d.ts",
      "import": "./dist/Table/index.js",
      "require": "./dist/Table/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "CircularLoader": [
        "./dist/CircularLoader/index.d.ts"
      ],
      "CopyToClipboard": [
        "./dist/CopyToClipboard/index.d.ts"
      ],
      "Details": [
        "./dist/Details/index.d.ts"
      ],
      "FileUpload": [
        "./dist/FileUpload/index.d.ts"
      ],
      "Form": [
        "./dist/Form/index.d.ts"
      ],
      "helpers": [
        "./dist/helpers/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "LinearLoader": [
        "./dist/LinearLoader/index.d.ts"
      ],
      "LoadingButton": [
        "./dist/LoadingButton/index.d.ts"
      ],
      "Page": [
        "./dist/Page/index.d.ts"
      ],
      "Snackbar": [
        "./dist/Snackbar/index.d.ts"
      ],
      "Table": [
        "./dist/Table/index.d.ts"
      ]
    }
  }
}