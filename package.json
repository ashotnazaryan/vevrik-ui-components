{
  "name": "@vevrik/ui-components",
  "version": "0.1.8",
  "description": "Reusable React UI components built with MUI",
  "author": {
    "name": "Ashot",
    "url": "https://github.com/ashotnazaryan/vevrik-ui-components"
  },
  "license": "MIT",
  "homepage": "https://github.com/ashotnazaryan/vevrik-ui-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashotnazaryan/vevrik-ui-components.git"
  },
  "bugs": {
    "url": "https://github.com/ashotnazaryan/vevrik-ui-components/issues"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./CircularLoader": {
      "types": "./dist/CircularLoader/index.d.ts",
      "import": "./dist/CircularLoader/index.js",
      "require": "./dist/CircularLoader/index.cjs"
    },
    "./CopyToClipboard": {
      "types": "./dist/CopyToClipboard/index.d.ts",
      "import": "./dist/CopyToClipboard/index.js",
      "require": "./dist/CopyToClipboard/index.cjs"
    },
    "./LinearLoader": {
      "types": "./dist/LinearLoader/index.d.ts",
      "import": "./dist/LinearLoader/index.js",
      "require": "./dist/LinearLoader/index.cjs"
    },
    "./LoadingButton": {
      "types": "./dist/LoadingButton/index.d.ts",
      "import": "./dist/LoadingButton/index.js",
      "require": "./dist/LoadingButton/index.cjs"
    },
    "./Snackbar": {
      "types": "./dist/Snackbar/index.d.ts",
      "import": "./dist/Snackbar/index.js",
      "require": "./dist/Snackbar/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "CircularLoader": [
        "./dist/CircularLoader/index.d.ts"
      ],
      "CopyToClipboard": [
        "./dist/CopyToClipboard/index.d.ts"
      ],
      "LinearLoader": [
        "./dist/LinearLoader/index.d.ts"
      ],
      "LoadingButton": [
        "./dist/LoadingButton/index.d.ts"
      ],
      "Snackbar": [
        "./dist/Snackbar/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "mui",
    "ui",
    "vevrik",
    "components",
    "library"
  ],
  "scripts": {
    "build": "npm run generate:exports && tsup && tsc",
    "generate:exports": "node ./scripts/generate-exports.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,html,md,yml,yaml,scss}\"",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "@mui/icons-material": "^6 || ^7",
    "@mui/material": "^6 || ^7",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,scss,md}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "rimraf": "^6.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
