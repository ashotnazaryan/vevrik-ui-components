{
  "name": "@vevrik/ui-components",
  "version": "0.1.5",
  "description": "Reusable React UI components built with MUI",
  "author": "Ashot",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.cjs",
      "types": "./dist/*/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "ui",
    "vevrik",
    "components",
    "library"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup src/index.ts src/*/index.ts --format cjs,esm --dts --outDir dist --clean",
    "lint": "eslint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,html,md,yml,yaml,scss}\"",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@mui/icons-material": "^6 || ^7",
    "@mui/material": "^6 || ^7",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,scss,md}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vite": "^7.1.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
